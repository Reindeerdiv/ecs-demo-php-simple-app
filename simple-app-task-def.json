{
    
    "containerDefinitions": [
        {
            "environment": [],
            "name": "simple-app",
            "image": "studentapp:v_%BUILD_NUMBER%",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                     "hostPort": 80,
                      "protocol": "tcp",
                        "containerPort": 80
                }
            ],
           
            "entryPoint": [
                "/usr/sbin/apache2",
                "-D",
                "FOREGROUND"
            ],
            "essential": true
        }
      
    ],
    "family": "student-1",
    
      "networkMode": "bridge",
       "taskRoleArn": "arn:aws:iam::950421538861:role/ecsTaskExecutionRole"
       
}
