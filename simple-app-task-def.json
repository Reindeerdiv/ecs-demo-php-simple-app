{
    "family": "student-1",
   
    "containerDefinitions": [
        {
            "environment": [],
            "name": "simple-app",
            "image": "studentapp:v_%BUILD_NUMBER%",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                     "hostPort": 80,
                      "protocol": "tcp",
                        "containerPort": 80
                }
            ],
           
            "entryPoint": [
                "/usr/sbin/apache2",
                "-D",
                "FOREGROUND"
            ],
            "essential": true
        }
      
    ],
      "networkMode": "awsvpc"
}
